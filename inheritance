
package com.oops;
//inheritance is always used for avoiding to write the code again and again
class Calc1
{
public int add(int a ,int b)
{
int c= a+b;
System.out.println(c);
return c;
}
}
class Calculator extends Calc1
{
public int Div(int a ,int b)
{
int c= b/a;
System.out.println(c);
return c;
}
public int Mult(int a ,int b)
{
int c= b*a;
System.out.println(c);
return c;
}
public int sub(int a ,int b)
{
int c= b-a;
System.out.println(c);
return c;
}

}
public class InheritanceExample {
public static void main(String[] args) {
//Calc1 obj1 = new Calc1();
//obj1.add(20, 10);
Calculator obj2 = new Calculator();
obj2.sub(10,10);
obj2.Div(10,10);
obj2.add(10, 10);
obj2.Mult(10,10);

}
}
